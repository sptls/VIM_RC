set number
set relativenumber
syntax on

set autoindent
set expandtab
set tabstop=2
set shiftwidth=2
"set noet ci pi sts=0 sw=4 ts=4
set showcmd
set showmode
set showmatch
set hlsearch
set nobackup
set wildmenu
set wildmode=longest:full,full

"inoremap {      {}<Left>
"inoremap {<CR>  {<CR>}<Esc>O<Tab>
"inoremap {{     {
"inoremap {}     {}

"inoremap (		()<Left>
"inoremap ))		<Esc><s-a>

"vnoremap {		c{<CR><Esc>pi}
